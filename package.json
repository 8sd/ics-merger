{
  "name": "@jacobmischka/ical-merger",
  "version": "2.4.0",
  "description": "Merges multiple ical .ics files into one for importing elsewhere",
  "main": "dist/ical-merger.cjs.js",
  "bundle": "dist/ical-merger.bundle.js",
  "module": "dist/ical-merger.js",
  "scripts": {
    "start": "node dist/server.js",
    "serve": "nodemon dist/server.js",
    "build": "npm-run-all clean --parallel build:*",
    "now-build": "npm-run-all clean --parallel build:webpack build:rollup",
    "build:webpack": "NODE_ENV=production webpack -p",
    "build:rollup": "run-p rollup:*",
    "build:components": "NODE_ENV=production babel ./src/components --out-dir ./dist/components",
    "build:helpers": "NODE_ENV=production babel ./src/constants.js -o ./dist/constants.js && NODE_ENV=production babel ./src/utils.js -o ./dist/utils.js",
    "rollup:node": "rollup -c",
    "rollup:bundle": "rollup -c rollup.config.bundle.js",
    "rollup:server": "rollup -c rollup.config.server.js",
    "watch": "npm-run-all clean --parallel watch:*",
    "watch:webpack": "webpack --progress --color --watch",
    "watch:rollup": "run-p rollup-watch:*",
    "watch:components": "NODE_ENV=production babel ./src/components --watch --out-dir ./dist/components",
    "watch:constants": "NODE_ENV=production babel ./src/constants.js --watch -o ./dist/constants.js",
    "watch:utils": "NODE_ENV=production babel ./src/utils.js --watch -o ./dist/utils.js",
    "rollup-watch:node": "rollup -cw",
    "rollup-watch:bundle": "rollup -c rollup.config.bundle.js -w",
    "rollup-watch:server": "rollup -c rollup.config.server.js -w",
    "clean": "rm -rf ./dist ./public ; mkdir -p dist",
    "analyze:bundle": "webpack-bundle-analyzer ./public/js/stats.json",
    "deploy": "echo '!.env.json' >> .npmignore ; now ; sed -i '$ d' .npmignore",
    "prepublish": "npm-run-all build"
  },
  "bin": {
    "ical-merge": "./cli.js"
  },
  "repository": {
    "url": "git@github.com:jacobmischka/ics-merger.git",
    "type": "git"
  },
  "engines": {
    "node": ">6.8.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 9"
  ],
  "author": "Jacob Mischka <jacob@mischka.me>",
  "license": "MIT",
  "dependencies": {
    "clipboard": "^1.5.15",
    "color": "^2.0.0",
    "color-string": "^1.4.0",
    "express": "^4.14.0",
    "fullcalendar": "^3.0.1",
    "ical.js": "^1.2.2",
    "iframe-resizer": "^3.5.14",
    "linkify-it": "^2.0.2",
    "lodash": "^4.17.4",
    "node-fetch": "^1.6.3",
    "prop-types": "^15.5.6",
    "react": "^15.5.3",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^15.5.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "styled-jsx": "^1.0.10",
    "typeface-noto-sans": "^0.0.35"
  },
  "devDependencies": {
    "apply-loader": "^2.0.0",
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "classlist-polyfill": "^1.0.3",
    "css-loader": "^0.28.0",
    "element-dataset": "^2.2.2",
    "es6-promise": "^4.0.5",
    "eslint": "^4.4.1",
    "eslint-plugin-react": "^7.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "raf": "^3.3.2",
    "rollup": "^0.47.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "style-loader": "^0.18.2",
    "styled-jsx-postcss": "^0.2.0",
    "url-search-params-polyfill": "^2.0.0",
    "webpack": "^3.5.4",
    "webpack-bundle-analyzer": "^2.1.1",
    "whatwg-fetch": "^2.0.0"
  },
  "icalMerger": {
    "prodid": "-//Jacob Mischka//iCal Merger//EN",
    "version": "2.0"
  }
}
