{
  "name": "ical-merger",
  "version": "1.0.0",
  "description": "Merges multiple ical .ics files into one for importing elsewhere",
  "main": "dist/ical-merger.cjs.js",
  "web": "dist/ical-merger.bundle.js",
  "jsnext:main": "dist/ical-merger.js",
  "module": "dist/ical-merger.js",
  "scripts": {
    "start": "node dist/server.js",
    "serve": "nodemon dist/server.js",
    "build": "run-p build:*",
    "build:webpack": "webpack -p",
    "build:sass": "gulp sass --production",
    "build:rollup": "run-p rollup:*",
    "rollup:node": "rollup -c",
    "rollup:bundle": "rollup -c rollup.config.bundle.js",
    "rollup:server": "rollup -c rollup.config.server.js",
    "watch": "run-p watch:*",
    "watch:sass": "gulp sass:watch",
    "watch:webpack": "webpack --progress --color --watch",
    "watch:rollup": "run-p rollup-watch:*",
    "rollup-watch:node": "rollup -cw",
    "rollup-watch:bundle": "rollup -c rollup.config.bundle.js -w",
    "rollup-watch:server": "rollup -c rollup.config.server.js -w",
    "analyze:bundle": "webpack-bundle-analyzer ./public/js/stats.json",
    "deploy": "now -e NODE_ENV=production",
    "now-build": "echo \"Built files are currently checked into source control\""
  },
  "bin": {
    "ical-merge": "./cli.js"
  },
  "repository": {
    "url": "git@github.com:jacobmischka/ics-merger.git",
    "type": "git"
  },
  "engines": {
    "node": ">6.8.1"
  },
  "author": "Jacob Mischka <jacobmischka@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0",
    "ical.js": "^1.2.2",
    "node-fetch": "^1.6.3"
  },
  "devDependencies": {
    "apply-loader": "^1.0.0",
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "balloon-css": "^0.4.0",
    "classlist-polyfill": "^1.0.3",
    "clipboard": "^1.5.15",
    "color": "^1.0.2",
    "color-string": "^1.4.0",
    "cssnano": "^3.8.0",
    "element-dataset": "^2.2.2",
    "es6-promise": "^4.0.5",
    "eslint": "^3.9.1",
    "eslint-plugin-react": "^6.6.0",
    "fullcalendar": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-postcss": "^6.2.0",
    "gulp-sass": "^3.0.0",
    "iframe-resizer": "^3.5.5",
    "linkify-it": "^2.0.2",
    "lodash": "^4.16.6",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "postcss-import": "^9.0.0",
    "react": "^15.3.2",
    "react-addons-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "^2.1.1",
    "whatwg-fetch": "^2.0.0"
  },
  "icalMerger": {
    "prodid": "-//Jacob Mischka//iCal Merger//EN",
    "version": "2.0"
  }
}
